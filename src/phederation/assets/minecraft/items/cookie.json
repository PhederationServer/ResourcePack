{
    "model":
    {
        "type": "select",
        "property": "custom_model_data",
        "fallback":
        {
            "type": "model",
            "model": "item/cookie"
        },
        "cases":
        [
            {
                "when": "gingerbread",
                "model":
                {
                    "type": "model",
                    "model": "custom:item/cookie/gingerbread"
                }
            },
            {
                "when": "sbm1",
                "model":
                {
                    "type": "model",
                    "model": "custom:item/cookie/sbm1"
                }
            },
            {
                "when": "sbm2",
                "model":
                {
                    "type": "model",
                    "model": "custom:item/cookie/sbm2"
                }
            },
            {
                "when": "sbm3",
                "model":
                {
                    "type": "model",
                    "model": "custom:item/cookie/sbm3"
                }
            }
        ]
    }
}